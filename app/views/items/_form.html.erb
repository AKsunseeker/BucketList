<%= form_for @item do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br />
    <div class="input-field browser-default" >
    <%= f.select :category, options_for_select([['Adventure'],['Travel'],['Family'], ['Education'], ['Service'], ['FUN'], ['Entertainment'], ['Misc']]) %>
    <%= f.label :category %>
  </div>
  <br />
  <%= f.submit %>
<% end %>